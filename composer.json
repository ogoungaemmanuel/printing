{
    "name": "xslain/laravel-printing",
    "description": "Direct printing for Laravel apps",
    "keywords": [
        "xslain",
        "laravel-printing",
        "PrintNode",
        "CUPS",
        "ipp",
        "Receipt printing",
        "Direct printing",
        "Raw printing"
    ],
    "homepage": "https://github.com/xslain/laravel-printing",
    "license": "MIT",
    "authors": [
        {
            "name": "Ogounga Emmanuel",
            "email": "info@xslain.com",
            "homepage": "https://xslain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^12.30.1",
        "mike42/escpos-php": "^4.0",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.1.0",
        "pestphp/pest-plugin-laravel": "^3.0.0",
        "php-http/message-factory": "^1.1",
        "php-http/socket-client": "^2.1",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "spatie/invade": "^2.1",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "Xslain\\Printing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xslain\\Printing\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/pest -p",
        "format": "vendor/bin/pint --dirty"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xslain\\Printing\\PrintingServiceProvider"
            ],
            "aliases": {
                "Printing": "Xslain\\Printing\\Facades\\Printing"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}